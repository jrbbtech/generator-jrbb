{
  "extends": [
    "stylelint-a11y/recommended",
    "stylelint-prettier/recommended"
  ],
  "plugins": [
    "stylelint-a11y",
    "stylelint-prettier",
    "stylelint-scss"
  ],
  "rules": {
    "a11y/media-prefers-reduced-motion": true,
    "a11y/no-outline-none": true,
    "a11y/selector-pseudo-class-focus": true,
    "prettier/prettier": true
  }
}
